{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js",
          ""
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "movie",
      "urls": [
        "https://api.themoviedb.org/3/*",
        "/assets/i18n/**",
        "/api/**",
        "http://localhost:3000/*",
        "https://image.tmdb.org/*"
      ],
      "cacheConfig": {
        "maxSize": 6,
        "maxAge": "12h",
        "timeout": "2s",
        "strategy": "freshness"
      }
    }
  ]
}